{% block pluszwei_faq_manager_index_search_suggest %}
    <div class="faq-search-result-container js-search-result">

        {% block pluszwei_faq_manager_index_search_suggest_content %}
            <ul class="faq-search-result-content">
                {% if articles|length == 0 %}

                    {% block pluszwei_faq_manager_index_search_suggest_no_result %}
                        <li class="faq-search-no-result">
                            {{ 'pluszweiFaqManager.search.noResult'|trans|sw_sanitize }}
                        </li>
                    {% endblock %}

                {% else %}
                    {% for article in articles %}

                        {% block pluszwei_faq_manager_index_search_suggest_item %}
                            <li>
                                <a class="item"
                                   href="{{ seoUrl('pluszwei.faq.frontend.detail', {articleId: article.id}) }}"
                                   rel="nofollow">
                                                <span class="caption">
                                                    <span class="main_title">{{ article.translated.title }}</span>
                                                    <span class="description">{{ article.translated.teaser }}</span>
                                                    <span class="sub_title">{{ 'pluszweiFaqManager.article.readingTime'|trans({'%min%': article.readingTimeInMin})|sw_sanitize }}</span>
                                                </span>
                                    <span class="icon">
                                    {% sw_icon 'arrow-right' %}
                                </span>
                                </a>
                            </li>
                        {% endblock %}

                    {% endfor %}
                {% endif %}
            </ul>
        {% endblock %}

    </div>
{% endblock %}