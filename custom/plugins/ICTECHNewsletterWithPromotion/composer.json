{
  "name": "ictech/newsletter-with-promotion",
  "description": "Newsletter Popup With Promotions",
  "type": "shopware-platform-plugin",
  "license": "proprietary",
  "version": "1.0.1",
  "authors": [
    {
      "name": "iCreative Technologies"
    }
  ],
  "autoload": {
    "psr-4": {
      "ICTECHNewsletterWithPromotion\\": "src/"
    }
  },
  "require": {
    "shopware/core": "~6.5.0",
    "shopware/storefront": "~6.5.0",
    "shopware/administration": "~6.5.0"
  },
  "extra": {
    "shopware-plugin-class": "ICTECHNewsletterWithPromotion\\ICTECHNewsletterWithPromotion",
