      "de-DE": "Newsletter-Popup mit Werbeaktionen",
      "en-GB": "Newsletter Popup With Promotions",
      "fr-FR": "Newsletter Popup avec promotions"
    },
    "manufacturerLink": {
      "de-DE": "https://www.icreativetechnologies.com",
      "en-GB": "https://www.icreativetechnologies.com",
      "fr-FR": "https://www.icreativetechnologies.com"
    }
  },
  "require-dev": {
    "nunomaduro/phpinsights": "^2.8"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
