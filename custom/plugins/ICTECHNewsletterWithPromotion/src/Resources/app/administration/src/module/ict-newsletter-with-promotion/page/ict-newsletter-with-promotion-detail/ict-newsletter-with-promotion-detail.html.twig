{% block ict_newsletter_with_promotion_newsletter_popup_detail %}
    <sw-page class="ict-newsletter-with-promotion-popup-detail">
        {% block sw_task_detail_header %}
            <template #smart-bar-header>
                <h2>{{ placeholder(popup, 'name', $tc('ict-newsletter-with-promotion.newsletter-popup.detail.textHeadline')) }}</h2>
            </template>

        {% endblock %}

        {% block ict_newsletter_with_promotion_newsletter_popup_detail_actions %}
            <template #smart-bar-actions>

                <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
                {% block ict_newsletter_with_promotion_newsletter_popup_detail_actions_abort %}
                    <sw-button
                            v-tooltip.bottom="tooltipCancel"
                            :disabled="popupIsLoading"
                            @click="onCancel"
                    >
                        {{ $tc('ict-newsletter-with-promotion.general.cancelButtonText') }}
                    </sw-button>
                {% endblock %}

                <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
                {% block ict_newsletter_with_promotion_newsletter_popup_detail_actions_save %}
                    <sw-button-process
                            v-model="isSaveSuccessful"
                            v-tooltip.bottom="tooltipSave"
                            class="newsletter-popup-detail__save-action"
                            :is-loading="isLoading"
                            variant="primary"
                            @click.prevent="onSave"
                    >
                        {{ $tc('ict-newsletter-with-promotion.general.saveButtonText') }}
                    </sw-button-process>
                {% endblock %}

            </template>
        {% endblock %}

        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block ict_newsletter_with_promotion_newsletter_popup_detail_language_switch %}
            <template #language-switch>
                <sw-language-switch
                        :disabled="popupId == null"
                        :save-changes-function="saveOnLanguageChange"
                        :abort-change-function="abortOnLanguageChange"
                        @on-change="onChangeLanguage"
                />
            </template>
        {% endblock %}

        {% block ict_newsletter_with_promotion_newsletter_popup_detail_content %}
            <template #content>
                <sw-card-view>
                    <template v-if="popupIsLoading">
                        <sw-skeleton variant="detail-bold" />
                        <sw-skeleton />
                    </template>

                    <template v-else>

