                                    :entity-description="placeholder(popup, 'name', $tc('sw-manufacturer.detail.textHeadline'))"
                            />
                        {% endblock %}

                        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
